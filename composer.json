{
	"name": "digitalsloth/znn-php",
	"description": "A basic PHP SDK for JSON-RPC endpoints of the zenon.network",
	"minimum-stability": "stable",
	"license": "mit",
	"authors": [
		{
			"name": "DigitalSloth",
			"email": "digitals1oth@proton.me"
		}
	],
	"require": {
		"php": ">=8.0.0",
		"ext-json": "*",
		"guzzlehttp/guzzle": "^7.0"
	},
	"require-dev": {
		"phpunit/phpunit": "^9.0"
	},
	"autoload": {
		"psr-4": {
			"DigitalSloth\\ZnnPhp\\": "src"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"DigitalSloth\\ZnnPhp\\Examples\\": "examples/src",
			"DigitalSloth\\ZnnPhp\\Tests\\": "tests"
		}
	}
}
