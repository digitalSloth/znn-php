{
    "name": "digitalsloth/znn-php",
    "description": "A PHP SDK for JSON-RPC endpoints of the zenon.network",
    "minimum-stability": "stable",
    "license": "mit",
    "authors": [
        {
            "name": "DigitalSloth",
            "email": "digitals1oth@proton.me"
        }
    ],
    "require": {
        "php": ">=8.0",
        "bitwasp/bech32": "^0.0.1",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^7.0",
        "simplito/elliptic-php": "^1.0",
        "textalk/websocket": "^1.5",
        "web3p/rlp": "^0.3.5",
        "web3p/web3.php": "^0.1.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "DigitalSloth\\ZnnPhp\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DigitalSloth\\ZnnPhp\\Examples\\": "examples",
            "DigitalSloth\\ZnnPhp\\Tests\\": "tests"
        }
    }
}
